{
    "config": {
        "vendor-dir": "vendor",
        "prepend-autoloader": false
    },
    "autoload": {
        "psr-4": {
            "PaypalPPBTlib\\": "vendor/ppbtlib/src/",
            "PaypalAddons\\": "."
        }
    },
    "scripts": {
        "pre-autoload-dump": [
            "if [ ${PPBTCLASSLIB_DEV_PATH} ]; then php ${PPBTCLASSLIB_DEV_PATH}/PPBTlib/refresh.php .; fi"
        ]
    },
    "require": {
      "paypal/paypal-checkout-sdk": " 1.*",
      "guzzlehttp/guzzle": "6.*"
    }
}
